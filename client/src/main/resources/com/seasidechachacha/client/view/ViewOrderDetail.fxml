<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Pagination?>

<VBox xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.seasidechachacha.client.controllers.ViewOrderDetailController">
    <children>
        <Label alignment="TOP_CENTER" prefHeight="30.0" prefWidth="803.0" styleClass="main-label" text="Chi tiết lịch sử tiêu thụ">
        </Label>
        <Pagination fx:id="paginationDetail" maxWidth="700.0">
            <VBox.margin>
                <Insets left="50.0" />
            </VBox.margin>
        </Pagination>
        <Button fx:id="btnBack" mnemonicParsing="false" onAction="#goBack" text="Quay lại">
            <VBox.margin>
                <Insets left="50.0" top="20.0" />
            </VBox.margin>
            <graphic>
                <FontAwesomeIconView fill="WHITE" glyphName="ARROW_LEFT" />
            </graphic>
        </Button>
    </children>
    <stylesheets>
        <URL value="@../css/style.css" />
    </stylesheets>
</VBox>
